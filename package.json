{
  "name": "n30phyte.github.io",
  "version": "0.1.0",
  "private": true,
  "homepacge": "https://n30phyte.github.io",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "build:css": "postcss src/**/*.css --base src --dir build/static/css",
    "start:css": "postcss -w src/**/*.css --base src --dir build/static/css",
    "build:js": "react-scripts build",
    "start:js": "react-scripts start",
    "build": "npm-run-all build:*",
    "start": "npm-run-all -p start:*",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.1.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1"
  }
}
